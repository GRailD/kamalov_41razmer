<Window x:Class="kamalov_41razmer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:kamalov_41razmer"
        mc:Ignorable="d"
        Title="Обувная Камалова" Height="700" Width="1200" MinHeight="700" MinWidth="1200" FontFamily="Comic Sans MS" Icon="Rex/icon.ico">
    <Grid Background="#FFFFCC99">
        <Grid.RowDefinitions>
            <RowDefinition Height="75"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>
        <Image Source="Rex/logo.png" HorizontalAlignment="Left"></Image>
        <TextBlock Text="41 размер ноги" FontSize="30" HorizontalAlignment="Center" VerticalAlignment="Center"></TextBlock>
        <Frame Grid.Row="1" Name="MainFrame" ContentRendered="MainFrame_ContentRendered" NavigationUIVisibility="Hidden"></Frame>
        <Button Content="Назад!" Name="BtnBack" Width="175" Height="30" HorizontalAlignment="Right" Margin="5"
                Click="BtnBack_Click"></Button>
    </Grid>
</Window>
